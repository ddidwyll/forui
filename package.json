{
  "name": "forui",
  "version": "0.0.1",
  "description": "Svelte UI kit",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.html,.svelte .",
    "fix": "eslint --fix --ext .js,.html,.svelte .",
    "format": "prettier --write --svelte-sort-order markup-scripts-styles --plugin-search-dir=. **/*.svelte; prettier --write **/*.js; prettier --write **/*.*css",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddidwyll/forui.git"
  },
  "keywords": [
    "svelte",
    "ui",
    "kit"
  ],
  "author": "Phil D'Oz <ddidwyll@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ddidwyll/forui/issues"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.6.0",
    "patch-package": "^6.2.0",
    "svelte": "^3.12.1"
  },
  "peerDependencies": {
    "svelte": "^3.0.0"
  },
  "homepage": "https://github.com/ddidwyll/forui#readme"
